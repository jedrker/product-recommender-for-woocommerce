# WooCommerce API Configuration
# Skopiuj ten plik jako .env i wypełnij swoimi danymi

# URL sklepu WooCommerce (bez końcowego slash)
WOOCOMMERCE_URL=https://twoj-sklep.pl

# Klucze API WooCommerce
# Wygeneruj w: WooCommerce > Ustawienia > Zaawansowane > REST API
WOOCOMMERCE_CONSUMER_KEY=ck_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WOOCOMMERCE_CONSUMER_SECRET=cs_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Opcjonalne ustawienia
# Czas cache'owania produktów w sekundach (domyślnie: 3600 = 1 godzina)
CACHE_DURATION=3600

# Maksymalna liczba produktów do pobrania (domyślnie: 100)
MAX_PRODUCTS=100

# Timeout dla zapytań API w sekundach (domyślnie: 30)
API_TIMEOUT=30

# Logowanie (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
